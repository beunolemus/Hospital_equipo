r\n      return this.item.children && this.item.children.length\r\n    },\r\n    hasGroupOpenSlot() {\r\n      return !!this.$slots.groupOpened\r\n    },\r\n    hasGroupCloseSlot() {\r\n      return !!this.$slots.groupClosed\r\n    },\r\n    hasItemSlot() {\r\n      return !!this.$slots.item\r\n    }\r\n  },\r\n  methods: {\r\n    toggle: function () {\r\n      if (this.isFolder) {\r\n        this.isOpen = !this.isOpen\r\n      }\r\n    },\r\n    makeFolder: function () {\r\n      if (!this.isFolder) {\r\n        this.$emit('make-folder', this.item)\r\n        this.isOpen = true\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAkEA,OAAOA,QAAO,MAAO,YAAW;AAEhC,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,KAAK,EAAE,CAAC,MAAM,EAAE,UAAU,CAAC;EAC3BC,QAAQ,EAAE;IACRH,QAAO,EAAPA;EACF,CAAC;EACDI,IAAI,WAAAA,KAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CAAC,CAAC,IAAI,CAACC;IACjB;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,QAAQ,EAAE,SAAAA,SAAA,EAAY;MACpB,OAAO,IAAI,CAACC,IAAI,CAACC,QAAO,IAAK,IAAI,CAACD,IAAI,CAACC,QAAQ,CAACC,MAAK;IACvD,CAAC;IACDC,gBAAgB,WAAAA,iBAAA,EAAG;MACjB,OAAO,CAAC,CAAC,IAAI,CAACC,MAAM,CAACC,WAAU;IACjC,CAAC;IACDC,iBAAiB,WAAAA,kBAAA,EAAG;MAClB,OAAO,CAAC,CAAC,IAAI,CAACF,MAAM,CAACG,WAAU;IACjC,CAAC;IACDC,WAAW,WAAAA,YAAA,EAAG;MACZ,OAAO,CAAC,CAAC,IAAI,CAACJ,MAAM,CAACJ,IAAG;IAC1B;EACF,CAAC;EACDS,OAAO,EAAE;IACPC,MAAM,EAAE,SAAAA,OAAA,EAAY;MAClB,IAAI,IAAI,CAACX,QAAQ,EAAE;QACjB,IAAI,CAACH,MAAK,GAAI,CAAC,IAAI,CAACA,MAAK;MAC3B;IACF,CAAC;IACDe,UAAU,EAAE,SAAAA,WAAA,EAAY;MACtB,IAAI,CAAC,IAAI,CAACZ,QAAQ,EAAE;QAClB,IAAI,CAACa,KAAK,CAAC,aAAa,EAAE,IAAI,CAACZ,IAAI;QACnC,IAAI,CAACJ,MAAK,GAAI,IAAG;MACnB;IACF;EACF;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}